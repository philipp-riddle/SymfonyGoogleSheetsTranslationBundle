{
    "name": "phiil/googlesheets-translation-bundle",
    "description": "This bundle enables you to deploy your translation within seconds and the best part: Manage them in Googlesheets, probably the easiest way.",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Philipp Martini",
            "email": "philipp@riddle.com"
        }
    ],
    "require": {
        "php": "^7.4|^8.0|^8.1",
        "symfony/framework-bundle": "^5.4",
        "symfony/translation": "^5.4",
        "symfony/cache": "^5.4"
    },
    "autoload": {
        "psr-4": {
            "Phiil\\GoogleSheetsTranslationBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Phiil\\GoogleSheetsTranslationBundle\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^5.4",
        "friendsofphp/php-cs-fixer": "^2.16"
    }
}
